<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="backup.wingos.com.wingbackup.MainActivity">


    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/main_icon"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="This is new Phone"
        android:id="@+id/newPhoneBtn"
        android:background="@drawable/main_text_background"
        android:textColor="@color/colorWhite"/>

    <Button
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="This is old Phone"
        android:id="@+id/oldPhoneBtn"
        android:background="@drawable/main_text_background"
        android:layout_marginTop="30dp"
        android:textColor="@color/colorWhite"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/testBtn"
        android:layout_marginTop="20dp"
        android:text="测试自定义View"/>


</LinearLayout>
